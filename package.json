{
  "name": "portfolio",
  "private": true,
  "description": "Fast, modern portfolio and blog powered by Gatsby",
  "version": "2.0.0",
  "author": "Stephen Vondenstein <stephen@vondenstein.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "check-format": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:ci": "jest --coverage"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@sentry/browser": "5.15.5",
    "gatsby": "2.21.21",
    "gatsby-alias-imports": "1.0.4",
    "gatsby-plugin-emotion": "4.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-plugin-theme-ui": "0.3.0",
    "gatsby-plugin-typography": "2.5.1",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-source-graphql": "2.5.1",
    "gatsby-transformer-remark": "2.8.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-typography": "0.16.19",
    "theme-ui": "0.3.1",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.7.0",
    "@testing-library/react": "10.0.4",
    "babel-jest": "25.5.1",
    "babel-plugin-emotion": "10.0.33",
    "babel-preset-gatsby": "0.4.1",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.5.4",
    "jest-emotion": "10.0.32",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1"
  },
  "keywords": [
    "gatsby"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vondenstein/portfolio"
  },
  "bugs": {
    "url": "https://github.com/vondenstein/portfolio/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
