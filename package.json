{
  "name": "portfolio",
  "private": true,
  "description": "Fast, modern portfolio and blog powered by Gatsby",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://vondenstein.com",
  "author": {
    "name": "Stephen Vondenstein",
    "email": "stephen@vondenstein.com",
    "url": "https://vondenstein.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vondenstein/portfolio"
  },
  "bugs": {
    "email": "support@vondenstein.com",
    "url": "https://github.com/vondenstein/portfolio/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\" --require-pragma",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@sentry/browser": "^5.15.5",
    "apollo-link-prismic": "^1.0.7",
    "gatsby": "^2.20.25",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-emotion": "^4.3.0",
    "gatsby-plugin-manifest": "^2.3.7",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sitemap": "^2.4.0",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typography": "^2.4.4",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-source-graphql": "^2.5.0",
    "gatsby-source-prismic-graphql": "^3.6.2",
    "gatsby-transformer-remark": "^2.8.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "theme-ui": "^0.3.1",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "babel-jest": "^25.5.1",
    "babel-plugin-emotion": "^10.0.33",
    "babel-preset-gatsby": "^0.4.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.3",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^10.2.1",
    "prettier": "2.0.4",
    "react-test-renderer": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": "prettier --write --require-pragma"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
